{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "matchy-dungeon",
  "mainBinaryName": "matchy-dungeon",
  "version": "0.1.0",
  "identifier": "com.matchy-dungeon.app",
  "plugins": {},
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "Matchy Dungeon",
        "width": 1280,
        "height": 720,
        "center": false,
        "fullscreen": false,
        "additionalBrowserArgs": "--disable-features=msWebOOUI,msPdfOOUI,msSmartScreenProtection --autoplay-policy=no-user-gesture-required",
        "resizable": true
      }
    ],
    "security": {
      "csp": null
    }
  }
}
